  $(document).ready(function() {
      $('#branches').select2({
          width: "274px",
          data: [{"text":"This Commit's Parents","children":[{"text":"dd085089^1 (78a0b1e7)","id":"/packages/GithubAdminTools/blobs/78a0b1e7818bbc5806241c8fc35302527b0c3702/--/templates/DevGuide_CC_MIT/LICENSE-SUMMARY"}]},{"text":"Official Branches","children":[{"text":"mainline (default)","id":"/packages/GithubAdminTools/blobs/heads/mainline/--/templates/DevGuide_CC_MIT/LICENSE-SUMMARY"}]}],
          createSearchChoice: function(term, data) {
            if ($(data).filter(function() { return this.text.localeCompare(term)===0; }).length===0) {
              // This code fires if the user enters a string and hits return (rather than selecting the item
              // from the dropdown.  This breaks when viewing commits (logs). Customize it accordingly.
              if ('/packages/GithubAdminTools/blobs/'.match(/\/logs/)) {
                var id_string = '/packages/GithubAdminTools/blobs//' + term;
                if ('templates/DevGuide_CC_MIT/LICENSE-SUMMARY') {
                  id_string += '/--/templates/DevGuide_CC_MIT/LICENSE-SUMMARY';
                }
                return {text:term, id: id_string};
              }
              return {text:term, id:'/packages/GithubAdminTools/blobs/' + term + '/--/templates/DevGuide_CC_MIT/LICENSE-SUMMARY'};
            }
          },
      });
      $('#branches').select2('data', null);
      $('#branches').change(function() {
        document.location = $(this).val();
      });
  });
</script>
